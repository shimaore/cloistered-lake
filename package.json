{
  "name": "@shimaore/lake",
  "version": "3.0.3",
  "devDependencies": {
    "@types/node": "^20.9.0",
    "ava": "5.3.1",
    "coffeescript": "2.7.0",
    "esbuild": "0.19.5",
    "typescript": "5.2.2"
  },
  "scripts": {
    "build": "tsc && mv index.js index.mjs && mv index.js.map index.mjs.map",
    "build:cjs": "esbuild --outfile=index.cjs index.ts --format=cjs",
    "pretest": "yarn build && egrep '^    ' README.md | sed -e 's/^    //' | sed -e 's:@shimaore/lake:./index.mjs:' > test.mjs",
    "test": "ava --verbose test.mjs",
    "test:ts": "cd test-ts && yarn && yarn test"
  },
  "description": "functional async generators toolbox",
  "type": "module",
  "module": "index.mjs",
  "main": "index.cjs",
  "types": "index.d.ts",
  "exports": {
    "require": "./index.cjs",
    "import": "./index.mjs",
    "types": "./index.d.ts"
  },
  "repository": "gitlab.com/shimaore/cloistered-lake",
  "author": "Stephane Alnet <stephane@shimaore.net>",
  "license": "MIT"
}
